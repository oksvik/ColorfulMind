<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dudar.colorfulmind">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RainbowActivity"
            android:label="@string/rainbow"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".AlphabetActivity"
            android:label="@string/alphabet"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".ColorslogicActivity"
            android:label="@string/logic_of_colors"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".colorlogic.ColorsLogicHelpActivity"
            android:parentActivityName=".ColorslogicActivity"/>
    </application>

</manifest>